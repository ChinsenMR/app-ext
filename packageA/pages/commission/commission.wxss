page {
  /* background-color: #FFF; */
}

.layout {
  position: relative;
}

.c-total {
  height: 230rpx;
  background: linear-gradient(#FF3333, #FF6666);
  color: #fff;
  font-size: 36rpx;
}

.c-total text:first-child {
  font-size: 70rpx;
  margin-bottom: 2rpx;
}

.c-nav {
  height: 110rpx;
  background-color: #FF3333;
  color: #fff;
  font-size: 24rpx;
}

.cn-item {
  position: relative
}

.cn-item::after {
  content: '';
  position: absolute;
  right: 0;
  top: 15rpx;
  height: 45rpx;
  width: 1rpx;
  background-color: #FF7575;
}

.cn-amount {
  font-size: 28rpx;
  margin-bottom: 5rpx;
}

.cu-header {
  color: #666;
  font-size: 30rpx;
  padding-left: 30rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  background-color: #F7F7F7;
}

.c-ul {
  background-color: #fff;
  margin: 60rpx 0 100rpx 0;
}

.c-li {
  height: 126rpx;
  border-bottom: 1rpx solid #F4F4F4;
  padding: 0 30rpx;
}

.c-order {
  color: #333;
  font-size: 30rpx;
}

.c-tip {
  color: #666;
  font-size: 26rpx;
  margin-left: 20rpx;
}

.c-time {
  color: #C5C3C3;
  font-size: 22rpx;
  margin-top: 3rpx;
}

.cu-amount {
  text-align: right;
  font-size: 34rpx;
  color: #FF3333;
}

.empty {
  color: #999;
  font-size: 28rpx;
  text-align: center;
  margin-top: 200rpx;
}

.addAcitve {
  width: 50%;
  font-size: 28rpx;
  color: #333;
  border-radius: 20rpx;
  margin: 0 auto;
  margin-top: -30rpx;
  position: absolute;
  left: 0;
  right: 0;
  margin-left: auto;
  margin-right: auto;
  border: 1rpx solid #eee;
}

/* 提现协议弹窗 */
.win_wrap {
  width: 80%;
  height: 1000rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  /* background-color: #fff; */
  z-index: 10999;
  box-shadow: 10px 10px 5px #888888;
  border-radius: 10rpx;
  border: 1rpx solid #eee;
}

.win_style {
  width: 100%;
  background-color: #fff;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
}

.win_main {
  width: 100%;
  height: 900rpx;
  background: rgba(0, 0, 0, .6);
  position: relative;
}

.search_btn {
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  border: 1rpx solid #fff;
  padding: 5rpx;
  box-sizing: border-box;
}

.btn_img {
  width: 100%;
  height: 100%;
  background-color: #fff;
  border-radius: 50%;
}

.btn_wrap {
  position: absolute;
  left: 25%;
  bottom: 0;
  font-size: 30rpx;
  color: #fff;
  display: flex;
  align-items: center;
  padding: 20rpx 0;
}

.zhezhao {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  /* background: rgba(244, 244, 244, .6); */
  z-index: 99;
}